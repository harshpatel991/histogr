{
  "name": "Histogr",
  "description": "A history visualizer",
  "version": "1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "app": {
    "launch": {
      "local_path": "html/window.html"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "history",
    "\u003Call_urls>",
    "webRequest",
    "webRequestBlocking"
  ]
}
