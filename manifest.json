{
  "name": "Histogr",
  "description": "A history visualizer",
  "version": "1",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "html/window.html"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "history"
  ]
}
